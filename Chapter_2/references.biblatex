	langid = {english},
	file = {Full Text:C\:\\Users\\ian.tsang\\Zotero\\storage\\XHKB6M3F\\Zhang et al. - 2022 - Genome Wide Identification of Respiratory Burst Ox.pdf:application/pdf},
}

